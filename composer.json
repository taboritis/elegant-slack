{
    "name": "taboritis/elegant-slack-php",
    "description": "Slack Message Build Kid written in PHP",
    "type": "library",
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "phpstan/phpstan": "^1.12",
        "squizlabs/php_codesniffer": "^3.10",
        "vlucas/phpdotenv": "^5.6",
        "symfony/var-dumper": "^7.1",
        "fakerphp/faker": "^1.23",
        "rector/rector": "^1.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Taboritis\\ElegantSlack\\": "src/"
        }
    },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
      "files": [
          "tests/functions.php"
      ]
  },
  "authors": [
        {
            "name": "Piotr Skrobol"
        }
    ],
    "minimum-stability": "stable"
}
