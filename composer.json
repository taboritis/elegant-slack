{
    "name": "taboritis/elegant-slack-messages",
    "description": "Elegant Messages in Slack",
    "type": "library",
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "phpstan/phpstan": "^1.12",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Taboritis\\ElegantSlackMessages\\": "src/"
        }
    },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
        {
            "name": "Piotr Skrobol"
        }
    ],
    "minimum-stability": "stable"
}
